{
  "name": "sns_app_api",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ts-node": "ts-node --files -r tsconfig-paths/register",
    "dev": "ts-node-dev --poll --respawn --transpile-only --files -r tsconfig-paths/register ./src/index.ts",
    "seed": "yarn ts-node seed.ts",
    "seed:dummy": "yarn ts-node seed-dummy.ts",
    "lint": "prettier --check src",
    "lint:fix": "prettier --write src",
    "typecheck": "tsc -p . --noEmit",
    "test": "jest --runInBand",
    "typeorm": "TS_NODE_TRANSPILE_ONLY=true yarn ts-node ./node_modules/typeorm/cli.js",
    "start": "node -r ./tsconfig-paths-bootstrap.js dist/src/index.js",
    "console": "NODE_OPTIONS=--experimental-repl-await yarn ts-node console.ts"
  },
  "dependencies": {
    "argon2": "^0.27.1",
    "axios": "^0.21.1",
    "boom": "^7.3.0",
    "class-validator": "^0.12.2",
    "cors": "^2.8.5",
    "dayjs": "^1.10.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "glob": "^7.1.6",
    "humps": "^2.0.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "open-graph-scraper": "^4.7.1",
    "reflect-metadata": "^0.1.13",
    "tsconfig-paths": "^3.9.0",
    "typeorm": "^0.2.29"
  },
  "devDependencies": {
    "@types/boom": "^7.3.0",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@types/glob": "^7.1.3",
    "@types/humps": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.13",
    "@types/open-graph-scraper": "^4.3.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "ts-node-dev": "1.0.0-pre.44",
    "typescript": "^4.1.3"
  }
}
